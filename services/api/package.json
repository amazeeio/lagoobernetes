{
  "name": "api",
  "version": "3.0.0",
  "description": "Lagoobernetes GraphQL API",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=10.16.3"
  },
  "scripts": {
    "format": "prettier-eslint --write '**/*.js'",
    "lint": "../../node-packages/eslint-config-lagoobernetes-node/node_modules/.bin/eslint .",
    "test": "jest --forceExit --detectOpenHandles --maxWorkers=10",
    "test:watch": "jest --forceExit --detectOpenHandles --maxWorkers=10 --watch",
    "build": "tsc --pretty",
    "start": "flow-node -r dotenv-extended/config dist",
    "dev": "NODE_ENV=development nodemon --ignore '*.test.ts' --watch src -e js,json,ts --exec 'tsc --sourceMap && flow-node --inspect=0.0.0.0:9229 -r dotenv-extended/config' -- dist",
    "sync:gitlab:users": "flow-node dist/gitlab-sync/users",
    "sync:gitlab:groups": "flow-node dist/gitlab-sync/groups",
    "sync:gitlab:projects": "flow-node dist/gitlab-sync/projects",
    "sync:gitlab:all": "yarn run sync:gitlab:users && yarn run sync:gitlab:groups && yarn run sync:gitlab:projects",
    "sync:opendistro-security": "flow-node --max-http-header-size=80000 dist/helpers/sync-groups-opendistro-security",
    "sync:bitbucket:repo-permissions": "flow-node dist/bitbucket-sync/repo-permissions"
  },
  "keywords": [],
  "author": "amazee.io <hello@amazee.io> (http://www.amazee.io)",
  "license": "MIT",
  "dependencies": {
    "@lagoobernetes/commons": "4.0.0",
    "apollo-server-express": "^2.2.5",
    "aws-sdk": "^2.378.0",
    "body-parser": "^1.18.2",
    "camelcase-keys": "^4.2.0",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "dotenv-extended": "^2.0.1",
    "elasticsearch": "^15.0.0",
    "es7-sleep": "^1.0.0",
    "express": "^4.16.1",
    "flow-remove-types": "^1.2.3",
    "got": "^9.2.2",
    "graphql": "^14.5.8",
    "graphql-iso-date": "^3.5.0",
    "graphql-list-fields": "^2.0.2",
    "graphql-middleware": "^4.0.2",
    "graphql-rabbitmq-subscriptions": "^1.1.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.6",
    "graphql-type-json": "^0.3.0",
    "jsonwebtoken": "^8.0.1",
    "keycloak-admin": "https://github.com/amazeeio/keycloak-admin.git#bd015d2e34634f262c0827f00620657427e3c252",
    "keycloak-connect": "^5.0.0",
    "knex": "^0.20.2",
    "mariasql": "^0.2.6",
    "moment": "^2.24.0",
    "morgan": "^1.9.0",
    "node-cache": "^4.2.1",
    "ramda": "^0.25.0",
    "snakecase-keys": "^1.2.0",
    "sshpk": "^1.14.2",
    "validator": "^10.8.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.18",
    "@types/ramda": "types/npm-ramda#dist",
    "axios": "^0.19.0",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.14.2",
    "prettier-eslint-cli": "^4.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.4"
  }
}
