  },
  "license": "MIT",
  "dependencies": {
    "@lagoobernetes/commons": "4.0.0",
    "amqp-connection-manager": "^1.3.5",
    "amqplib": "^0.5.1",
    "bl": "^1.2.1",
    "buffer-equal-constant-time": "^1.0.1",
    "ramda": "^0.25.0",
    "uuid4": "^1.0.0"
  },
  "devDependencies": {
    "flow-remove-types": "^1.2.3",
    "jest": "^24.9.0",
    "newman": "^4.5.4",
    "nodemon": "^1.12.1"
  }
}
